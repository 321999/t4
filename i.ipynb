def normalize_columns(df):
    df.columns = (
        df.columns
        .str.strip()
        .str.lower()
        .str.replace(" ", "_")
    )
    return df
def time_to_seconds(t):
    if pd.isna(t):
        return 0
    h, m, s = map(int, str(t).split(":"))
    return h * 3600 + m * 60 + s

